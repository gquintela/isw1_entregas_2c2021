!Portfolio methodsFor: 'adding' stamp: 'JD 10/16/2021 12:45:08' prior: 50873802!
addAReceptiveAccount: aReceptiveAccount
	
	self assert: (receptiveAccounts includes: aReceptiveAccount) not description: [self signalReceptiveAccountAlreadyContained].
	
	self assert: (portfolios allSatisfy: [ :aPortfolio | aPortfolio notIncludesReceptiveAccount: aReceptiveAccount ]) description: [self signalReceptiveAccountAlreadyContained].
	
	portfolioContainer isNil ifFalse: [
		self assert: (portfolioContainer canAddAReceptiveAccount: aReceptiveAccount) description: [self signalReceptiveAccountAlreadyContained]
	].
	
	self assert: (portfolios allSatisfy: [ :aPortfolio | (aPortfolio portfolios) allSatisfy: [ :aGrandchildPortfolio | aGrandchildPortfolio notIncludesReceptiveAccount: aReceptiveAccount] ]) description: [self signalReceptiveAccountAlreadyContained].

	receptiveAccounts add: aReceptiveAccount.! !

