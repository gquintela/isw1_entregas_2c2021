addAPortfolio: aPortfolioToAdd

	self assert: (self ~= aPortfolioToAdd) description: [self signalPortfolioAlreadyContained].
	
	self assert: (self notIncludesAnyReceptiveAccountOf: aPortfolioToAdd) description: [self signalPortfolioAlreadyContained].
	
	self assert: (aPortfolioToAdd notIncludesPortfolio: self) description: [self signalPortfolioAlreadyContained].
	
	self assert: (portfolios allSatisfy: [ :aChildPortfolio | aChildPortfolio notIncludesPortfolio: aPortfolioToAdd ]) description: [self signalPortfolioAlreadyContained].
	
	portfolioContainer isNil ifFalse: [
		self assert: (portfolioContainer canAddAPortfolio: aPortfolioToAdd ) description: [self signalPortfolioAlreadyContained]
	].
	
	self assert: (portfolios allSatisfy: [ :aChildPortfolio | (aChildPortfolio portfolios) allSatisfy: [ :aGrandchildPortfolio | aGrandchildPortfolio notIncludesPortfolio: aPortfolioToAdd ]]) description: [self signalPortfolioAlreadyContained].
	
	self assert: (portfolios allSatisfy: [ :aChildPortfolio | aChildPortfolio notIncludesAnyReceptiveAccountOf: aPortfolioToAdd ]) description: [self signalPortfolioAlreadyContained].

	aPortfolioToAdd portfolioContainer: self. 

	portfolios add: aPortfolioToAdd.! !

