test06CannotListAnItemWithAnInvalidCartId

	| aRestInterface anInvalidCartId |
	
	aRestInterface := self defaultRestInterface.
	
	anInvalidCartId := 0.
	self
		should: [aRestInterface listCart: anInvalidCartId]
