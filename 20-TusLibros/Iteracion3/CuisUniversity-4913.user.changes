	aRestInterface := self defaultRestInterface.
	aCartId := 	aRestInterface createCartFor: testObjectsFactory validUser with: testObjectsFactory validPassword.
	aRestInterface addToCart: aCartId quantity: 1 of: testObjectsFactory itemSellByTheStore.
	anotherCartId := 	aRestInterface createCartFor: testObjectsFactory validUser with: testObjectsFactory validPassword.

	cartContents := aRestInterface listCart: aCartId.
