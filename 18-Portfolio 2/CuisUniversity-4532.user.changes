!PortfolioStructureCalculator methodsFor: 'as yet unclassified' stamp: 'JD 10/24/2021 21:57:59' prior: 50884752!
visitPortfolio: aPortfolioToVisit
	
	| aPortfolioStructureCalculator |
	
	aPortfolioToVisit ~= portfolio ifTrue: [
		aPortfolioStructureCalculator := PortfolioStructureCalculator for: aPortfolioToVisit.
		portfolioChildsStructure := portfolioChildsStructure, aPortfolioStructureCalculator calculate.
	]
! !
!PortfolioStructureCalculator methodsFor: 'as yet unclassified' stamp: 'JD 10/24/2021 21:59:17' prior: 50885188!
visitPortfolio: aPortfolioToVisit
	
	| aPortfolioStructureCalculator |
	
