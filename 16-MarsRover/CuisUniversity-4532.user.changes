!MarsRover methodsFor: 'process commands' stamp: 'FR 10/5/2021 22:43:52' prior: 50885250!
moveBy: aSequenceOfCommands
	
		| aCommand index |
		index := 1.
		aCommand := aSequenceOfCommands first .
		
		(self isValid: aCommand) whileTrue: [
			aCommand = $f ifTrue: [self moveForward].
			aCommand = $b ifTrue: [self moveBackward].
			aCommand = $l ifTrue: [self rotateLeft].
			aCommand = $r ifTrue: [self rotateRight].
			aCommand := aSequenceOfCommands at: index.
			index := index + 1.
		]
! !

