test18CanCalculateTheStructureOfAPortfolioWithAReceptiveAccountAndAnotherPortfolio
	
	| aFamilyPortfolio aFamilyPortfolioName aChildsPortfolio aChildsPortfolioName anOwnReceptiveAccountName anOwnReceptiveAccount aJuanReceptiveAccountName aJuanReceptiveAccount anExpectedChildsPortfolioStructure anExpectedFamilyPortfolioStructure |
	
	aJuanReceptiveAccountName := 'Juan Account'.
	aJuanReceptiveAccount := ReceptiveAccount named: aJuanReceptiveAccountName.
	aChildsPortfolioName := 'Childs Portfolio'.
	aChildsPortfolio := Portfolio named: aChildsPortfolioName with: aJuanReceptiveAccount.
	
	anOwnReceptiveAccountName := 'Own Account'.
	anOwnReceptiveAccount := ReceptiveAccount named: anOwnReceptiveAccountName.
	aFamilyPortfolioName := 'Family Portfolio'.
	aFamilyPortfolio := Portfolio named: aFamilyPortfolioName with: anOwnReceptiveAccount with: aChildsPortfolio.

	anExpectedChildsPortfolioStructure := (OrderedCollection with: aChildsPortfolioName with: (OrderedCollection with: aJuanReceptiveAccountName)).
	anExpectedFamilyPortfolioStructure := (OrderedCollection with: aFamilyPortfolioName with: (OrderedCollection with: anOwnReceptiveAccountName with: anExpectedChildsPortfolioStructure)).

	self assertThatPortfolioStructureFor: aFamilyPortfolio is: anExpectedFamilyPortfolioStructure 
	! !

