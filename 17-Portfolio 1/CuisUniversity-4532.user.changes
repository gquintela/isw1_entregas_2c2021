addAPortfolio: aPortfolioToAdd

	self assert: (self ~= aPortfolioToAdd) description: [self signalPortfolioAlreadyContained].
	
	self assert: (receptiveAccounts includesAnyOf: aPortfolioToAdd receptiveAccounts) not description: [self signalPortfolioAlreadyContained].
	
	self assert: (aPortfolioToAdd portfolios includes: self) not description: [self signalPortfolioAlreadyContained].
	
	self assert: (portfolios anySatisfy: [ :aChildPortfolio | aChildPortfolio portfolios includes: aPortfolioToAdd ]) not description: [self signalPortfolioAlreadyContained].
	
	portfolioContainer isNil ifFalse: [
		self assert: (portfolioContainer canAddAPortfolio: aPortfolioToAdd ) description: [self signalPortfolioAlreadyContained]
	].
	
	self assert: (portfolios anySatisfy: [ :aChildPortfolio | (aChildPortfolio portfolios) anySatisfy: [ :aGrandchildPortfolio | aGrandchildPortfolio portfolios includes: aPortfolioToAdd ]]) not description: [self signalPortfolioAlreadyContained].

	aPortfolioToAdd portfolioContainer: self. 

	portfolios add: aPortfolioToAdd.! !
